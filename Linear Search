//Linear Search

#include <iostream>

using namespace std;

int main()
{
    int n,i=0,f=0,k;
    cout<<"Enter Size \n";
    cin>>n;
    int a[n];
    
    cout<<"Enter Array \n";
    for(i=0;i<n;i++)
    cin>>a[i];
    
    cout<<"Enter key \n";
    cin>>k;
    
    for(i=0;i<n;i++)
    {
        if(a[i]==k)
        f=1;
    }
    if(f==1)
    cout<<"found";
    else
    cout<<"not Found";

    return 0;
}
